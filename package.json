{
  "name": "sinap-ide",
  "version": "0.0.0",
  "description": "Sinap is not a plot.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/2graphic/sinap-ide"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build && rimraf ./dist && rimraf ./dll",
    "webpack": "webpack --progress --profile --colors --display-error-details --display-cached",
    "format": "tsfmt -r",
    "build": "npm run format && npm run webpack",
    "build:dll": "npm run webpack -- --config webpack.dll.config.js",
    "build:prod": "npm run format && npm run webpack -- --env.ENV=production",
    "build:watch": "npm run format && npm run webpack -- --watch",
    "start": "electron build",
    "build:start": "npm run build && npm run start",
    "build:watch:start": "concurrently \"npm run build:watch\" \"npm run start\"",
    "electron-packager": "electron-packager build Sinap --out=dist --no-prune --arch=all --overwrite --icon=./app/images/icons/icon",
    "prepackage": "npm run clean && npm run build:dll && npm run build:prod && cpx package.json build",
    "package": "npm run prepackage && npm run electron-packager -- --platform=all",
    "package:darwin": "npm run prepackage && npm run electron-packager -- --platform=darwin",
    "package:win": "npm run prepackage && npm run electron-packager -- --platform=win32",
    "package:linux": "npm run prepackage && npm run electron-packager -- --platform=linux"
  },
  "dependencies": {
    "@angular/common": "2.4.x",
    "@angular/compiler": "2.4.x",
    "@angular/core": "2.4.x",
    "@angular/forms": "2.4.x",
    "@angular/platform-browser": "2.4.x",
    "@angular/platform-browser-dynamic": "2.4.x",
    "@angular/upgrade": "2.4.x",
    "core-js": "2.4.x",
    "reflect-metadata": "^0.1.x",
    "reset-css": "2.2.x",
    "rxjs": "5.0.x",
    "sinap-core": "0.0.25",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@types/electron": "1.4.30",
    "@types/node": "6.0.x",
    "angular2-template-loader": "0.6.x",
    "concurrently": "3.1.x",
    "rimraf": "2.5.x",
    "cpx": "1.5.x",
    "css-loader": "0.26.x",
    "electron": "1.4.8",
    "electron-packager": "8.4.x",
    "extract-loader": "0.1.x",
    "file-loader": "0.9.x",
    "html-loader": "0.4.x",
    "style-loader": "0.13.x",
    "to-string-loader": "1.1.x",
    "ts-loader": "1.3.x",
    "typescript": "2.1.x",
    "typescript-formatter": "4.0.x",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "webpack": "2.2.x",
    "webpack-merge": "2.6.x"
  }
}