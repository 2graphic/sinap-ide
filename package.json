{
  "name": "sinap-ide",
  "version": "0.0.0",
  "description": "Sinap is not a plot.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/2graphic/sinap-ide"
  },
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf ./build && rimraf ./dist && rimraf ./dll",
    "webpack": "webpack --progress --profile --colors --display-error-details --display-cached",
    "format": "tsfmt -r && tslint --project . --fix",
    "lint": "tsfmt --verify && tslint --project .",
    "build": "npm run format && npm run webpack",
    "build:dll": "npm run webpack -- --config webpack.dll.config.js",
    "build:prod": "npm run format && npm run webpack -- --env.ENV=production",
    "build:watch": "npm run format && npm run webpack -- --watch",
    "start": "electron . --enable-logging --remote-debugging-port=9222",
    "build:start": "npm run build && npm run start",
    "build:watch:start": "concurrently \"npm run build:watch\" \"npm run start\""
  },
  "dependencies": {
    "@angular/common": "4.0.0",
    "@angular/compiler": "4.0.0",
    "@angular/core": "4.0.0",
    "@angular/forms": "4.0.0",
    "@angular/http": "4.0.0",
    "@angular/platform-browser": "4.0.0",
    "@angular/platform-browser-dynamic": "4.0.0",
    "@types/archiver": "^0.15.37",
    "@types/extract-zip": "^1.6.2",
    "@types/ncp": "^2.0.0",
    "@types/tmp": "0.0.32",
    "angular-resizable-element": "1.0.0",
    "archiver": "^1.3.0",
    "core-js": "2.4.1",
    "extract-zip": "^1.6.0",
    "ncp": "^2.0.0",
    "ng-inline-svg": "3.0.0",
    "reflect-metadata": "0.1.10",
    "reset-css": "2.2.0",
    "rimraf": "2.6.1",
    "rxjs": "5.2.0",
    "tmp": "0.0.31",
    "sinap-core": "3.9.4",
    "sinap-typescript": "0.4.12",
    "sinap-types": "0.3.5",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@types/electron": "1.4.34",
    "@types/node": "7.0.10",
    "@types/rimraf": "0.0.28",
    "angular2-template-loader": "0.6.2",
    "concurrently": "3.4.0",
    "cpx": "1.5.0",
    "css-loader": "0.27.3",
    "electron": "1.6.2",
    "electron-packager": "8.6.0",
    "extract-loader": "0.1.0",
    "file-loader": "0.10.1",
    "glob": "^7.1.1",
    "html-loader": "0.4.5",
    "node-sass": "4.5.1",
    "sass-loader": "6.0.3",
    "source-map-loader": "^0.2.1",
    "source-map-support": "0.4.14",
    "style-loader": "0.16.0",
    "to-string-loader": "1.1.5",
    "ts-loader": "2.0.3",
    "tslint": "4.5.1",
    "typescript": "2.2.1",
    "typescript-formatter": "5.1.2",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "webpack": "2.2.1",
    "webpack-merge": "4.1.0",
    "zip-folder": "^1.0.0"
  }
}
