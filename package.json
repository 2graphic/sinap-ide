{
  "name": "sinap-ide",
  "version": "0.0.0",
  "description": "Sinap is not a plot.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/2graphic/sinap-ide"
  },
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf ./build && rimraf ./dist && rimraf ./dll",
    "webpack": "webpack --progress --profile --colors --display-error-details --display-cached",
    "format": "tsfmt -r && tslint --project . --fix",
    "lint": "tsfmt --verify && tslint --project .",
    "build": "npm run format && npm run webpack",
    "build:dll": "npm run webpack -- --config webpack.dll.config.js",
    "build:prod": "npm run format && npm run webpack -- --env.ENV=production",
    "build:watch": "npm run format && npm run webpack -- --watch",
    "start": "electron . --enable-logging --remote-debugging-port=9222",
    "build:start": "npm run build && npm run start",
    "build:watch:start": "concurrently \"npm run build:watch\" \"npm run start\""
  },
  "dependencies": {
    "@angular/common": "4.0.3",
    "@angular/compiler": "4.0.3",
    "@angular/core": "4.0.3",
    "@angular/forms": "4.0.3",
    "@angular/http": "4.0.3",
    "@angular/platform-browser": "4.0.3",
    "@angular/platform-browser-dynamic": "4.0.3",
    "@types/archiver": "0.15.37",
    "@types/extract-zip": "1.6.2",
    "@types/ncp": "2.0.0",
    "@types/tmp": "0.0.32",
    "angular-resizable-element": "1.0.0",
    "archiver": "1.3.0",
    "core-js": "2.4.1",
    "extract-zip": "1.6.0",
    "ncp": "2.0.0",
    "reflect-metadata": "0.1.10",
    "reset-css": "2.2.0",
    "rimraf": "2.6.1",
    "rxjs": "5.3.0",
    "tmp": "0.0.31",
    "sinap-core": "3.11.0",
    "sinap-typescript-loader": "0.4.18",
    "sinap-python-loader": "0.0.3",
    "sinap-types": "0.4.0",
    "zone.js": "0.8.8"
  },
  "devDependencies": {
    "@types/electron": "1.4.37",
    "@types/node": "7.0.13",
    "@types/rimraf": "0.0.28",
    "angular2-template-loader": "0.6.2",
    "concurrently": "3.4.0",
    "css-loader": "0.28.0",
    "electron": "1.6.5",
    "electron-osx-sign": "^0.4.4",
    "electron-packager": "8.6.0",
    "extract-loader": "0.1.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "glob": "7.1.1",
    "html-loader": "0.4.5",
    "material-design-icons": "3.0.1",
    "node-sass": "4.5.2",
    "sass-loader": "6.0.3",
    "source-map-loader": "0.2.1",
    "source-map-support": "0.4.14",
    "style-loader": "0.16.1",
    "to-string-loader": "1.1.5",
    "ts-loader": "2.0.3",
    "tslint": "4.5.0",
    "typescript": "2.2.2",
    "typescript-formatter": "5.1.3",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "webpack": "2.2.1",
    "webpack-material-design-icons": "0.1.0",
    "webpack-merge": "4.1.0"
  }
}
